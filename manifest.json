{
  "manifest_version": 2,
  "name": "GitHub Helpers",
  "version": "0.0.1",
  "description": "A set of helper functions for working with GitHub",

  "applications": {
    "gecko": {
      "id": "github-helpers-addon@example.com",
      "strict_min_version": "45.0"
    }
  },

  "content_scripts": [
    {
      "matches": ["https://github.com/*/pull/*/files*"],
      "js": ["config.js", "pr_helpers.js"]
    }
  ]
}